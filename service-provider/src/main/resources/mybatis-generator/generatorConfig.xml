<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<!--修改为mysql-connector-java-**.jar路径-->
	<classPathEntry	location="D:\Idea\Pro\mybatis-generator-core-1.3.2\lib\mysql-connector-java-5.1.35.jar"/>
	<context id="DB2Tables"	targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressDate" value="true"/>
			<property name="suppressAllComments" value="true"/>
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://192.168.145.10:3306/durian?useUnicode=true&amp;characterEncoding=utf8" userId="app" password="123456">

		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false"/>
		</javaTypeResolver>
		
		<javaModelGenerator targetPackage="com.example.serviceprovider.mysql.domain" targetProject="src/main/java">
		<!-- <javaModelGenerator targetPackage="com.durian.domain" targetProject="src/main/java"> -->
			<property name="enableSubPackages" value="true"/>
			<property name="trimStrings" value="true"/>
		</javaModelGenerator>
		
		<sqlMapGenerator targetPackage="com.example.serviceprovider.mysql.mapper" targetProject="src/main/java">
		<!-- <sqlMapGenerator targetPackage="com.durian.dao" targetProject="src/main/java"> -->
			<property name="enableSubPackages" value="true"/>
		</sqlMapGenerator>
		
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.example.serviceprovider.mysql.mapper" targetProject="src/main/java">
		<!-- <javaClientGenerator type="XMLMAPPER" targetPackage="com.durian.dao" targetProject="src/main/java"> -->
			<property name="enableSubPackages" value="true"/>
		</javaClientGenerator>
		
		<table tableName="user_log" domainObjectName="UserLog"
			enableCountByExample="true" enableUpdateByExample="true" enableDeleteByExample="true" enableSelectByExample="true" selectByExampleQueryId="true"
			enableSelectByPrimaryKey="true" enableDeleteByPrimaryKey="true" 
			>
		</table> 
		
	</context>
</generatorConfiguration>
